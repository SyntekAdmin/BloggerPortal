@section pagespecific {


    <script type="text/javascript">

    $(document).ready(function () {
        new Vue({
            el: '#app',
            data: {
                layout: 'grid',
                myorders: []
            },
            mounted: function () {
                this.load()
            },
            methods: {
                load: function () {
                    var that = this;

                    $.get('/content/data/influencers/faq.json', function (data) {
                        console.log(data);

                        $.each(data, function (i, v) {
                            if (v) Vue.set(that.myorders, i, v);
                        });

                    });

                }
            }
        });
    });

    </script>
}